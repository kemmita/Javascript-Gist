JS***************************************

function checkSpeed() {
    var speed = document.getElementById("speed").value;
if (speed>70) {
    window.alert("you are going to fast");
}else if(speed<40){
    window.alert("your speed is too slow.");
}
else{
    window.alert("your speed is fine.");
}


}
function defineSeason(){
    var s = document.getElementById("season").value;
    if(s == "winter"){
        window.alert("Es ist sehr kalt!");
    }
    else if(s == "fall"){
        window.alert("it is now wrestling season");
    }
    else if(s == "summer"){
        window.alert("Let's go to the beach");
        
    }
    else if(s == "spring"){
        window.alert("it is a bit cold at the beach");
    }
    else{
        window.alert("please check your spelling!");
    }
}
