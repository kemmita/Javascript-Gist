JS*****************************************
var year = ["2005", "1728", "1982", "1581"];
var ages = [];
var i = 0;
for (i = 0; i < year.length; i++) {
    ages[i] = 2016 - year[i];
    
}
document.getElementById("mypar").innerHTML += "Ages" +  "<br/>" +  ages;
for (i = 0; i < ages.length; i++){
    if(ages[i] > 17){
        document.getElementById("mypar2").innerHTML += " " +ages[i];
    }
    else{
        document.getElementById("mypar3").innerHTML += " " + ages[i];
    }
}

