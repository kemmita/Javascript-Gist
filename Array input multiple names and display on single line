        <p>Enter name 2:<input id="names2"></p>
        <p>Enter name 3:<input id="names3"></p>
        <p>Enter name 4:<input id="names4"></p>
        <button onclick="display()">Submit</button>
        <p id="mypar"></p>
        <script type="text/javascript" src="code.js"></script>
    
