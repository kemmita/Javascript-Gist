JS***************************************
function display() {
    var name1 = document.getElementById("names1").value;
    var name2 = document.getElementById("names2").value;
    var name3 = document.getElementById("names3").value;
    var name4 = document.getElementById("names4").value;
    
    
    
    
    var names = [name1, name2, name3, name4];
    document.getElementById("mypar").innerHTML = names;
}
