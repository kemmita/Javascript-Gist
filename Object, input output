JS********************************

function start(){//need function to print 
var person = {//create object
name: "Default",
LastName: "Default"
    
}
var fName = document.getElementById("jsi").value;//input from html
person.name = fName;//assigning html to bject.name
var lName = document.getElementById("jsi2").value;
person.LastName = lName;


document.getElementById("mypar").innerHTML = person.name+ person.LastName;//printing input
}
